{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(js|css|json|png|jpg|jpeg|gif|svg|ico|webp|woff2?)$",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
